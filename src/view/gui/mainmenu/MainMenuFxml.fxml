<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="1024.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" fillWidth="false" StackPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" text="New Game" fx:id="newGameButton"/ onAction="#openGameView">
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" text="Load Game" />
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" text="Options" />
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" text="Exit" />
         </children>
      </VBox>
   </children>
</StackPane>
